{
  "name": "@xagentauth/edge-cf",
  "version": "0.2.1",
  "description": "AgentAuth server adapter for Cloudflare Workers",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@xagentauth/core": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4",
    "typescript": "^5.7",
    "vitest": "^3"
  }
}
