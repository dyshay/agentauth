# Required â€” at least 32 characters
AGENTAUTH_SECRET=change-me-to-a-real-secret-at-least-32-bytes

# Store backend: memory | redis | postgres
STORE_BACKEND=redis

# Redis (when STORE_BACKEND=redis)
REDIS_URL=redis://localhost:6379

# Postgres (when STORE_BACKEND=postgres)
DATABASE_URL=postgres://agentauth:agentauth@localhost:5432/agentauth

# Feature flags
POMI_ENABLED=true
TIMING_ENABLED=true

# Server
PORT=3000
